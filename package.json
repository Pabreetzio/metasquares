{
  "name": "metasquares",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm run dev --workspace=@metasquares/web",
    "build:web": "npm run build --workspace=@metasquares/web",
    "dev:mobile": "npm run start --workspace=@metasquares/mobile",
    "install:all": "npm install"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
